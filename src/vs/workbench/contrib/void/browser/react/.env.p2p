# P2P Backend Configuration
# Frontend connection to lean P2P backend server

# Backend URLs
REACT_APP_P2P_HTTP=http://localhost:3000
REACT_APP_P2P_WS=ws://localhost:3001

# Room and User Configuration
REACT_APP_P2P_ROOM_ID=octatecode-collab
REACT_APP_P2P_USER_ID=
REACT_APP_P2P_USER_NAME=Anonymous

# Feature Flags
REACT_APP_P2P_ENABLED=true
REACT_APP_WEBRTC_ENABLED=true
REACT_APP_PERSISTENCE_ENABLED=true

# Backend Server (for npm start in p2p-backend/)
PORT=3000
SIGNALING_PORT=3001
NODE_ENV=development
CORS_ORIGINS=http://localhost:3000,http://localhost:8080,http://localhost:5173
